    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.9.7"
  }
}
