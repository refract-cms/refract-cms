{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "preserveSymlinks": true
  }
}
